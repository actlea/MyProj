//添加广告监测代码
var js_date=new Date();
var ad_day=js_date.getDate();
var ad_mouth=js_date.getMonth();
var ad_time_str=(ad_mouth+1)+"_"+ad_day;
function in_array(a,c){
  for(var b=0;b<c.length;b++){
    var thisEntry=c[b].toString();
    if(thisEntry==a)
    {
      return true;
      break;
    }
  }
  return false;
}

var pv_add_time_2 = new Array("11_15");
var pv_add_time_3 = new Array("11_19","11_20","11_21","11_22","11_23");
var pv_add_time_4 = new Array("11_5","11_8","11_15","11_29");


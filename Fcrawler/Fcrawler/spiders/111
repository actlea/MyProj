(function(){
    function _c(url){
        var c = document.createElement("script");
        c.setAttribute('type','text/javascript');
        c.setAttribute('src',url);
        c.setAttribute("charset",'UTF-8');
        document.body.appendChild(c);
    }
    function run(){
            _c("http://mat1.gtimg.com/pinglun/tx/js/subscribe_v3.js");
            _c("http://mat1.gtimg.com/pinglun/tx/js/lazysub_v3.js");
            /*
            var l = document.createElement("link");
            l.setAttribute("rel",'stylesheet');
            l.setAttribute('type','text/css');
            l.setAttribute('href','http://mat1.gtimg.com/pinglun/tx/css/lazysub_v3.css');
            document.body.appendChild(l);
            */
    }
    if(!window._subscribe_supported){
        if(!!window.addEventListener){
            window.addEventListener("load",run);
        }else{
            window.attachEvent("onload",run);
        }
        _subscribe_supported=true;
    }
})();

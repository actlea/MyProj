<script type="text/javascript">
// shenshenyan 2014-10-08 15:29  line : 102 <% if(list[i].competitionName != '中超') {%>
template.helper("$subtime", function(res){
    var time = res.substr(8,8).split(" ");
    return time[0] + "日" + time[1];
});
template.helper("$len", function(res){
    return res > 20 ? 20: res;
});

/**
* 视频直播 看比赛订阅
* @name    loadZB
* @author  jianminlu
* @update  2013-12-13 10:30
*/
var loadZB = {
    siteUrl: "http://sportswebapi.qq.com",
    loadFlag: true,
    loadData: function(o){
        var _this = this;
        $.ajax({
            url: this.siteUrl + "/user/attendMatch?matchId=" + o.matchId + "&competitionId=" + o.competitionId + "&type=" + o.type + "&callback=&t=",
            dataType: 'jsonp',
            success: function(data){
                if(data[0] == 4){
                    userLogin();
                    window.loginAllCallBack = function(){
                        _this.loadData(o);
                    };
                }else{
                    if(o.type == 1){
                        $(o.obj).attr({"title":"订阅成功"}).addClass("dy-on");
                    }else{
                        $(o.obj).attr({"title":"订阅直播赛果提醒"}).removeClass("dy-on");
                    }
                }
            }
        });
    },
    checkData:function (o){
        $.ajax({
            url: this.siteUrl + "/pinMatch/hasAttendMatches?mixedMatchIds=" + o.competitionId + "_" + o.matchId + "&callback=&t=",
            dataType: 'jsonp',
            success: function(data){
                if(data[1][0].hasAttend == 1){
                    $(o.obj).attr({"title":"订阅成功"}).addClass("dy-on");
                }else{
                    $(o.obj).attr({"title":"订阅直播赛果提醒"}).removeClass("dy-on");
                }
            }
        });
    },
    init: function(){
        var _this = this;
        var index = 0;
        var num = 5;
        var left = 0;

        $.getScript("http://mat1.gtimg.com/apps/test2/web_videoLive_all.json?t=" + new Date().getTime(), function(){
            if(all.length == 0){
                $("#mod-spzb").hide();
                return;
            }
            var data ={ list:all };
            var html = template.render('obj-zhibo', data);
            $("#mod-spzb .list").html(html);
            var zb = $("#mod-spzb .item-zb");
            if(loadZB.loadFlag){
                var item = $("#mod-spzb .item");
                var len = item.length;
                var itemW = item.outerWidth() + parseInt(item.css("marginRight"));

                $("#mod-spzb .list").width(itemW * item.length);
                item.hover(function(){
                    $(this).addClass("item-hover");
                }, function(){
                    $(this).removeClass("item-hover");
                });

                $("#mod-spzb .next").live("click", function(){
                    index ++;
                    if(index * num <= len - num){
                        left = itemW * num * index;
                    }else{
                        index --;
                        left = itemW * (len - 5);
                    }
                    $("#mod-spzb .list").animate({left: -left});
                });
                $("#mod-spzb .prev").live("click", function(){
                    index --;
                    if(index * num >= 0){
                        left = itemW * num * index;
                    }else{
                        index ++;
                        left = 0;
                    }
                    $("#mod-spzb .list").animate({left: -left});
                });
                $("#mod-spzb .item").each(function(index, obj){
                    var len = $("#mod-spzb .item-zb").length;
                    if(len > 3){
                        $("#mod-spzb .list").animate({left: -itemW*2});
                    }
                });
                $("#mod-spzb .ico-dy").each(function(index, obj){
                    loadZB.checkData({
                        matchId: $(obj).attr("matchId"),
                        competitionId: $(obj).attr("competitionId"),
                        obj: obj
                    })
                });
                $("#mod-spzb .ico-dy").live("click", function(){
                    var type = 1;
                    if($(this).hasClass("dy-on")){
                        type = 0;
                    }
                    loadZB.loadData({
                        matchId: $(this).attr("matchId"),
                        competitionId: $(this).attr("competitionId"),
                        type: type,
                        obj: this
                    });
                });
                loadZB.loadFlag = false;
            }
            if(zb.length > 0){
                //setTimeout(loadZB.init, 5000);
            }
        });
    }
}
loadZB.init();
</script><!--[if !IE]>|xGv00|f6d5cc3af2ff8435822511f2cb4a7de0<![endif]-->

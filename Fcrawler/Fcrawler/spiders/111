<script type="text/javascript">
function loadScript(sUrl, callback) {
    var _script = document.createElement('script');
    _script.setAttribute('type', 'text/javascript');
    _script.setAttribute('src', sUrl);
    document.getElementsByTagName('head')[0].appendChild(_script);
}
function cleartime() {}
var plInnerhtml = K('nn').html();
var save_obj = null,
save_area = null;
function showArea(obj) {
    if (!obj.isShow) {
        KK(".mod_A").each(function(obj,i){
            if(obj.find(".plArea")){
                obj.find(".plArea").item(0).remove()
            }
        })
        save_area = null;
        var plparentArea = K(obj).parent(2).append("<div id='Comment-Article-QQ' class='plArea'></div>");
        var plArea = plparentArea.last();
        plArea.html("<div class='closeBtn'><a href='javascript:void(0);'></a></div><div id='plContent' class='plContent'>" + plInnerhtml + "</div>");
        var newsId = K(obj).sibling( - 1).html();
        newsId = newsId.match(/\d+/g)[0];
        var newsContent = K(obj).sibling( - 2).html();
        plArea.first().click(function() {
            plArea.remove();
            obj.isShow = false;
            save_obj = null;
            save_area = null;
        });
        if (save_obj != null && save_obj != obj) {
            save_obj.isShow = false;
            if (save_area) save_area.remove();
        }
        obj.isShow = true;
        save_obj = obj;
        save_area = plArea;
        loadScript("http://sum.comment.gtimg.com.cn/php_qqcom/gsum.php?site=sports&c_id=" + newsId);
        document.domain = 'qq.com';
        cmt_site = 'sports';
        cmt_id = newsId;
        cmt_is_group = 0;
        cmt_count_id = 'comment_count|comment_count2';
        ARTICLE_INFO = window.ARTICLE_INFO || {
            site: 'sports',      // 频道id
            site_cname: '体育', // 频道名
            temp_type: 'default',
            article_ver: '1.2.4',
            title: newsContent,
            id: '20120718000827',
            pubtime: '2012年07月18日07:00',
            type: '1',
            article_url: '',
            sosokeys: {
                key1: '下一代iPhone'
            },
            catalog: 'tencentnews',
            sub_nav: '02new',
            topic: {
                name: '',
                cname: ''
            }
        }
        ARTICLE_INFO.title = newsContent;
        loadScript("http://tech.qq.com/2012/2012keji/MiniCmtTech_v1.1.js");
        TencentArticl.site = 'sports';
        TencentArticl.cmtId = newsId;
        TencentArticl.onload();
    } else {
        save_area.remove();
        obj.isShow = false;
        save_obj = obj;
        save_area = plArea;
    }
}

/* 评论列帖 s */
function _cbSum(data) {
    if (typeof data !== "undefined") {
        TencentArticl.$("comNum4").innerHTML = arguments[1]; //参与
        TencentArticl.$("comNum3").innerHTML = arguments[0]; //总贴数
        TencentArticl.$("cmt_3").href = "http://" + TencentArticl.cmtBase + "." + TencentArticl.site + ".qq.com/comment.htm?site=" + TencentArticl.site + "&id=" + TencentArticl.cmtId + "";
        TencentArticl.$("cmt_3").target = "_blank";
        if (arguments[2] > 0 && arguments[3] !== 0) { //如果精华帖不为空
            TencntART.JsLoader.load("http://" + TencentArticl.cmtBase + "." + TencentArticl.site + ".qq.com/php_qqcom/greplyBycid.php?site=" + TencentArticl.site + "&c_id=" + TencentArticl.cmtId + "&t_c=5&o_c=0&t=" + Math.round(Math.random() * 10000),
            function() {
                setTimeout("_replyBycid", 0);
            });
            TencentArticl.$("ComTitle").style.display = "block";
        } else {
            TencentArticl.$(TencentArticl.comtList).style.display = "none";
            TencentArticl.$("ComTitle").style.display = "none";
        }
    }
}
function getSibling(el) { //FF
    var prev = el.previousSibling;
    while (prev.nodeType != 1) {
        prev = prev.previousSibling;
    }
    return prev;
}
function _replyBycid(data) {
    var tempHTML = "";
    if (typeof data !== "undefined") {
        var l = data.hot_list.length;
        l = l > 5 ? 5 : l;
        for (var i = 0; i < l; i++) {
            if (i == l - 1) {
                if (isNaN(parseInt(data.hot_list[i].province_city))) {
                    tempHTML += "<li style='border-bottom:0px;'><span class='member'>腾讯" + data.hot_list[i].province_city + "网友 <a href='http://" + TencentArticl.cmtBase + "." + TencentArticl.site + ".qq.com/comment_user2.htm?uin=" + data.hot_list[i].uin + "' target='_blank'>" + data.hot_list[i].nick + "</a></span>";
                    if (data.hot_list[i].subflag == 1) {
                        tempHTML += "<p>" + data.hot_list[i].reply_content + "<a href=http://" + TencentArticl.cmtBase + "." + TencentArticl.site + ".qq.com/comment.htm?site=" + TencentArticl.site + "&id=" + TencentArticl.cmtId + " target='_blank'>[详细]</a></p>";
                    } else {
                        tempHTML += "<p>" + data.hot_list[i].reply_content + "</p>";
                    }
                } else {
                    tempHTML += "<li style='border-bottom:0px;'><span class='member'>腾讯网友 <a href='http://" + TencentArticl.cmtBase + "." + TencentArticl.site + ".qq.com/comment_user2.htm?uin=" + data.hot_list[i].uin + "' target='_blank'>" + data.hot_list[i].nick + "</a></span>";
                    if (data.hot_list[i].subflag == 1) {
                        tempHTML += "<p>" + data.hot_list[i].reply_content + "<a href=http://" + TencentArticl.cmtBase + "." + TencentArticl.site + ".qq.com/comment.htm?site=" + TencentArticl.site + "&id=" + TencentArticl.cmtId + " target='_blank'>[详细]</a></p>";
                    } else {
                        tempHTML += "<p>" + data.hot_list[i].reply_content + "</p>";
                    }
                }
            } else {
                if (isNaN(parseInt(data.hot_list[i].province_city))) {
                    tempHTML += "<li><span class='member'>腾讯" + data.hot_list[i].province_city + "网友 <a href='http://" + TencentArticl.cmtBase + "." + TencentArticl.site + ".qq.com/comment_user2.htm?uin=" + data.hot_list[i].uin + "' target='_blank'>" + data.hot_list[i].nick + "</a></span>";
                    if (data.hot_list[i].subflag == 1) {
                        tempHTML += "<p>" + data.hot_list[i].reply_content + "<a href=http://" + TencentArticl.cmtBase + "." + TencentArticl.site + ".qq.com/comment.htm?site=" + TencentArticl.site + "&id=" + TencentArticl.cmtId + " target='_blank'>[详细]</a></p>";
                    } else {
                        tempHTML += "<p>" + data.hot_list[i].reply_content + "</p>";
                    }
                } else {
                    tempHTML += "<li style='border-bottom:0px;'><span class='member'>腾讯网友 <a href='http://" + TencentArticl.cmtBase + "." + TencentArticl.site + ".qq.com/comment_user2.htm?uin=" + data.hot_list[i].uin + "' target='_blank'>" + data.hot_list[i].nick + "</a></span>";
                    if (data.hot_list[i].subflag == 1) {
                        tempHTML += "<p>" + data.hot_list[i].reply_content + "<a href=http://" + TencentArticl.cmtBase + "." + TencentArticl.site + ".qq.com/comment.htm?site=" + TencentArticl.site + "&id=" + TencentArticl.cmtId + " target='_blank'>[详细]</a></p>";
                    } else {
                        tempHTML += "<p>" + data.hot_list[i].reply_content + "</p>";
                    }
                }
            }
        }
    }
    TencentArticl.$(TencentArticl.comtList).innerHTML = tempHTML;
}
/* 评论列帖 e */
</script>

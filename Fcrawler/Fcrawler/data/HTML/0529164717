    <!--voteIndex ent-->
    <script type='text/javascript'>      

    $(".olympicsTops tr:odd").addClass("odd");
  
    function toupiao(vid,type){
        if ( !_common.isLogin() ) {
            commonLogin();
            return false;
        };

      var chk_value = '';
      var vote_url = $("#vote_url").val();
      if(type==0){
          chk_value = $("input[name='vote']:checked").val();
      }else{
          var obj=document.getElementsByName('vote');

          for(var i=0; i<obj.length; i++){
              if(obj[i].checked)
                  chk_value+=obj[i].value+"|";  //如果选中，将value添加到变量s中
          }
      }
      
      $.getJSON('http://snsvote.hupu.com/InsertVote_new.php?enews=InsertVote&voteid='+vid+'&vote='+chk_value+'&jsoncallback=?',
          function (json){
          var result = json.code==1 ? '投票成功' : json.info;
          $('#vote_info').html(result);
      });
      }
    </script>
    
